<script>
  var facilityTitleCirclText;
  var teamTitleCirclText;
  var KolelTitleTitleCirclText;
  var bottomKolelTitleCirclText;
  function makeTextCircle() {
    facilityTitleCirclText = new CircleType(
      document.getElementById("facility-title")
    ).radius(550);

    teamTitleCirclText = new CircleType(document.getElementById("team-title"))
      .dir(-1)
      .radius(550);

    KolelTitleTitleCirclText = new CircleType(
      document.getElementById("kolel-title")
    ).radius(550);

    bottomKolelTitleCirclText = new CircleType(
      document.getElementById("bottom-kolel-title")
    )
      .dir(-1)
      .radius(550);
  }

  function refreshTextCircle() {
    // console.log(facilityTitleCirclText);
    facilityTitleCirclText.refresh();
    teamTitleCirclText.refresh();
    KolelTitleTitleCirclText.refresh();
    bottomKolelTitleCirclText.refresh();
    // console.log("refresh", new Date().getTime());
  }
</script>
